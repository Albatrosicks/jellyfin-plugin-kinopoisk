    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "publish-to-run",
            "program": "${workspaceFolder:workspace-jellyfin}/Jellyfin.Server/bin/Debug/net5.0/jellyfin.dll",
            "args": [],
            "cwd": "${workspaceFolder:workspace-jellyfin}/Jellyfin.Server",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            // "serverReadyAction": {
            //     "action": "openExternally",
            //     "pattern": "Overriding address\\(es\\) \\'(https?:\\S+)\\'",
            // }
        },
