        //     "preLaunchTask": "build",
        //     "program": "${workspaceFolder}/src/Jellyfin.Plugin.Kinopoisk/bin/Debug/net5.0/Jellyfin.Plugin.Kinopoisk.dll",
        //     "args": [],
        //     "cwd": "${workspaceFolder}/src",
        //     "console": "internalConsole",
        //     "stopAtEntry": false
        // },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
